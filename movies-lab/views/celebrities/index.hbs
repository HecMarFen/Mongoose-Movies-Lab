<h2>Celebrities are displayed here</h2>
<ol>
    {{#each celebrities}}
        <li>
            <a href="/celebrities/{{_id}}">{{celName}}</a>
        </li>
        <form action="/celebrities/{{_id}}/delete" method="post">
             <button type="submit"> Delete </button>
        </form>
        <form action="/celebrities/{{_id}}/edit" method="post">
             <button type="submit"> Edit </button>
        </form>
    {{/each}}
</ol>
<div>
    <a href="/celebrities/newCelebrity">Create a new celebrity</a>
</div>
    

